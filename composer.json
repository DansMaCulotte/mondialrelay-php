{
    "name": "dansmaculotte/mondialrelay-php",
    "description": "PHP SDK for MondialRelay Web Services",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "mondialrelay",
        "mondialrelay-webservices"
    ],
    "homepage": "https://github.com/DansMaCulotte/mondialrelay-php",
    "authors": [
        {
            "name": "Martin Potel",
            "email": "martin@dansmaculotte.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "DansMaCulotte\\MondialRelay\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-soap": "*",
        "zendframework/zend-soap": "^2.8",
        "spatie/opening-hours": "^2.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^8.2"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "coverage": "@php vendor/bin/phpunit --coverage-html coverage",
        "format": "@php vendor/bin/php-cs-fixer fix --config .php_cs.php"
    },
    "config": {
        "sort-packages": true
    }
}
